<Project>
  <Import Project="..\version.props" />

  <PropertyGroup>
    <Product>Qowaiv</Product>
    <Description>Qowaiv implements common, universal domain objects. These types form the base of your domain model.</Description>
    <Authors>Ad van der Hoeven;Corniel Nobel;Jack Kester;Vincent Lesierse;Wilko Frieke</Authors>
    <Owners>Qowaiv community</Owners>
    <PackageTags>qowaiv domain model</PackageTags>
    <Company>Qowaiv community</Company>
    <Copyright>Copyright Â© Qowaiv community 2013-2017</Copyright>
    
    <PackageIconUrl>https://raw.githubusercontent.com/Corniel/Qowaiv/master/design/qowain-logo_nuget_064x064.gif</PackageIconUrl>
    <PackageProjectUrl>http://www.github.com/corniel/Qowaiv</PackageProjectUrl>
    <PackageLicenseUrl>http://www.github.com/corniel/Qowaiv/blob/master/LICENSE.txt</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Qowaiv/Qowaiv</RepositoryUrl>
    
    <VersionSuffix Condition="'$(VersionSuffix)'!='' AND '$(BuildNumber)' != ''">$(VersionSuffix)-$(BuildNumber)</VersionSuffix>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)Qowaiv.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

</Project>
